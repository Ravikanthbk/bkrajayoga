<p id="notice"><%= notice %></p>

<div class="well history-content">
<h3>Photos List   
  <p class="pull-right">
    <%= link_to 'Add New Photo (+)', new_admin_photo_path, :class=>"btn btn-default active" %>
  </p>    
</h3>
<div class="sortable">
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Photo</th>    
      <th>Title</th>
      <th>URL attachecd?</th>   
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="admin_carousels">
  <% if @photos.any? %>  
    <%= render @photos %>
  <% else %>
      <tr><td colspan="6">No records found. %></td></tr>  
    <% end %>  
  </tbody>
</table>
</div>
</div>
