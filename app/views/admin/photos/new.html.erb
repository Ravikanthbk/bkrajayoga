<div class="well history-content">
  <h2 style="color: #922582">Upload Photo</h2>

<%= form_for @photo do |f| %>
    <%= render partial: 'error_messages', object: @photo %>

  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image, class: 'form-control'%>
  </div>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: 'form-control' %>
  </div>


  <%= f.submit 'Submit', class: 'btn btn-primary' %>
  <%= link_to 'Canel', admin_photos_path, class: 'btn btn-primary' %>
<% end %>
</div>
