<%= simple_form_for(@admin_venue) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone %>
    <%= f.input :fax %>
    <%= f.input :email %>
    <%= f.input :city_locality %>
    <%= f.input :postcode %>
    <div class="form-group string required admin_venue_country">
      <label class="string required control-label" for="admin_venue_country"><abbr title="required">*</abbr> Country</label>
      <%= f.country_select :country, ['SG'] %>
    </div>
    <%= f.input :remarks %>
  </div>

  <div class="form-actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>

